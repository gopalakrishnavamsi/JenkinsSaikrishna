<aura:component extensible="true" abstract="true" controller="CLMApexUtility">
    <!--toast attributes-->
    <aura:attribute name="toastTitleText" type="String" access="private"/>
    <aura:attribute name="toastVariant" type="String" access="private"/>
    <aura:handler name="init" value="{!this}" action="{!c.onInit}"/>
    <aura:attribute name="namespace" type="string" default="c"/>
    <aura:attribute name="isClmAdmin" type="Boolean"/>
    {!v.body}
</aura:component>