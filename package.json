{
  "name": "salesforcecore",
  "version": "1.0.0",
  "description": "This is the new DocuSign for Salesforce base package. This is a lighter version of DFS that is installable on Salesforce Essentials organizations. This package also contains the Apex Toolkit.",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  },
  "scripts": {
    "lint": "eslint ./; exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sfdx-auth": "sfdx force:auth:web:login -a DocuDevHub -d",
    "sfdx-buildOrg": "sfdx force:org:create -f ./etc/dev.json -s -a dfsle-scratch-org",
    "sfdx-updateOrg": "sfdx force:source:push",
    "sfdx-postInstall": "sfdx force:apex:execute -f ./etc/postinstall.apex -u dfsle-scratch-org",
    "sfdx-generatePassword": "sfdx force:user:password:generate -u dfsle-scratch-org",
    "sfdx-openOrg":"sfdx force:org:open",
    "sfdx-setup":"npm run sfdx-auth && npm run sfdx-buildOrg && npm run sfdx-updateOrg && npm run sfdx-postInstall && npm run sfdx-generatePassword && npm run sfdx-openOrg"    
  },
  "repository": {
    "type": "git",
    "url": "https://github.docusignhq.com/Integrations/SalesforceCore.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
